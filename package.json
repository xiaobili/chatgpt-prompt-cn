{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "chatgpt-prompt-cn",
  "title": "CHATGPT PROMPT CN",
  "description": "Prompt list for interacting with ChatGPT",
  "icon": "command-icon.png",
  "author": "xiaobili",
  "owner": "billyjr",
  "categories": [
    "Data"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "prompt",
      "title": "prompt",
      "subtitle": "GPT3 Prompts Search",
      "description": "OpenAI GPT3 Prompt List With Chinese",
      "mode": "view",
      "preferences": [
        {
          "data": [
            {
              "title": "Paste to Active App",
              "value": "paste"
            },
            {
              "title": "Copy to Clipboard",
              "value": "copy"
            }
          ],
          "default": "copy",
          "description": "The primary action to perform on a selected prompt.",
          "name": "primaryAction",
          "required": false,
          "title": "Primary Action",
          "type": "dropdown"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.49.2",
    "axios": "^1.3.5"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}